<%= form_for(@manifest, html: { class: 'ui form' }) do |f| %>
  <% if @manifest.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@manifest.errors.count, "error") %> prohibited this manifest from being saved:</h2>

      <ul>
      <% @manifest.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :@id, '@id' %><br>
    <%= f.text_field :@id %>
  </div>
  <div class="field">
    <%= f.label :@type, '@type' %><br>
    <%= f.text_field :@type %>
  </div>
  <div class="field">
    <%= f.label :@context, '@context' %><br>
    <%= f.text_field :@context %>
  </div>
  <div class="field">
    <%= f.label :label %><br>
    <%= f.text_field :label %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
